   LSR A           JSR PRINTHN           PLA           AND #$0FPRINTHN           ORA #$30           CMP #$3A           BCC NOLETTER           ADC #6NOLETTER           JMP $FFD2           .BEND;---------------------------------------;WAIT UNTIL RASTER LINE IS IN BORDER;TO PREVENT GETTING DISTURBED BY ƒÕ¡SWAITBORDER           .BLOCK           LDA $D011           BMI OKWAIT           LDA $D012           CMP #30           BCS WAITOK           RTS           .BEND;---------------------------------------;WAIT FOR A KEY AND CHECK FOR ”‘œ–WAITKEY           .BLOCK           JSR $FD15           JSR $FDA3           CLIWAIT           JSR $FFE4           BEQ WAIT           CMP #3           BEQ STOP           RTSSTOP           LDA TURBOASS           BEQ LOAD           JMP $8000LOAD           JSR PRINT           .BYTE 13           .TEXT "BREAK"           .BYTE 13,0           JMP LOADNEXT           .BEND;---------------------------------------THISNAME   .NULL ""NEXTNAME   .NULL "-"MAIN;---------------------------------------           .BLOCK           .BEND;---------------------------------------           RTS